<Window x:Class="Glasac23.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Glasac23"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1200">
    <Grid >
       
        <Grid.Background>
            <ImageBrush ImageSource="/images/walpers/mainWalper.png"/>
        </Grid.Background>

        <Grid.RowDefinitions>
            <RowDefinition Height="150"/>
            <RowDefinition/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0" Text="GLASAC23" FontSize="100"
                   FontWeight="Bold" FontFamily="Impact" Foreground="#FFFF6900"
                   Margin="40 20 0 0" Grid.ColumnSpan="2"/>

        <StackPanel Orientation="Horizontal" Grid.ColumnSpan="2"
                    Grid.Row="2" Background="#BF929292"/>

        <Frame Grid.Row="1" x:Name="Okvir" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
